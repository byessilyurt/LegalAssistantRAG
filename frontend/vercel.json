{
    "functions": {
        "api/*.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        }
    ],
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "build": {
        "env": {
            "NODE_VERSION": "18.x"
        }
    }
}